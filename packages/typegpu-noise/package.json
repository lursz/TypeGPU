{
  "name": "@typegpu/noise",
  "type": "module",
  "version": "0.0.3",
  "description": "A set of noise/pseudo-random functions for use in WebGPU/TypeGPU apps.",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.mjs",
        "import": "./dist/index.mjs",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["README.md", "package.json", "dist/**/*"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test:types": "pnpm tsc --p ./tsconfig.json --noEmit && pnpm tsc --p ./tsconfig.test.json --noEmit",
    "publish": "echo \"Use pnpm prepare-package instead!\" && exit 1",
    "prepare-package": "tgpu-dev-cli prepack"
  },
  "keywords": [],
  "license": "MIT",
  "peerDependencies": {
    "typegpu": "^0.3.2"
  },
  "devDependencies": {
    "@webgpu/types": "^0.1.51",
    "@typegpu/tgpu-dev-cli": "workspace:*",
    "rollup-plugin-typegpu": "workspace:*",
    "typegpu": "workspace:*",
    "typescript": "^5.7.2",
    "tsup": "^8.0.2"
  }
}
